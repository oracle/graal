<!DOCTYPE filesystem PUBLIC "-//NetBeans//DTD Filesystem 1.2//EN" "http://www.netbeans.org/dtds/filesystem-1_2.dtd">
<filesystem>
    <folder name="Actions">
        <folder name="CallStack">
            <attr name="SystemFileSystem.localizingBundle" stringvalue="org.graalvm.visualizer.source.resources.Bundle"/>
        </folder>
    </folder>
    <folder name="IGV">
        <folder name="Languages">
            <folder name="Java">
                <attr name="mimetype" stringvalue="text/x-java"/>
                <attr name="SystemFileSystem.localizingBundle" stringvalue="org.graalvm.visualizer.source.resources.Bundle"/>
                <attr name="iconBase" stringvalue="org/graalvm/visualizer/source/resources/lang-truffle.png"/>
            </folder>
            <folder name="ruby">
                <attr name="displayName" bundlevalue="org.graalvm.visualizer.source.resources.Bundle#IGV/Languages/ruby"/>
                <attr name="mimetype" stringvalue="text/x-ruby"/>
                <attr name="iconBase" stringvalue="org/graalvm/visualizer/source/resources/lang-ruby.png"/>
            </folder>
            <folder name="js">
                <attr name="mimetype" stringvalue="text/javascript"/>
                <attr name="SystemFileSystem.localizingBundle" stringvalue="org.graalvm.visualizer.source.resources.Bundle"/>
                <attr name="iconBase" stringvalue="org/graalvm/visualizer/source/resources/lang-js.png"/>
            </folder>
            <folder name="r">
                <attr name="mimetype" stringvalue="text/x-r"/>
                <attr name="SystemFileSystem.localizingBundle" stringvalue="org.graalvm.visualizer.source.resources.Bundle"/>
                <attr name="iconBase" stringvalue="org/graalvm/visualizer/source/resources/lang-r.png"/>
            </folder>
            <folder name="python">
                <attr name="mimetype" stringvalue="text/x-python"/>
                <attr name="SystemFileSystem.localizingBundle" stringvalue="org.graalvm.visualizer.source.resources.Bundle"/>
                <attr name="iconBase" stringvalue="org/graalvm/visualizer/source/resources/lang-python.png"/>
            </folder>
        </folder>
    </folder>
    <folder name="Editors">
        <folder name="Keybindings">
            <folder name="NetBeans">
                <folder name="Defaults">
                    <file name="org-graalvm-visualizer-source-keybindings.xml" url="sourceview-keybindings.xml"/> 
                </folder>
            </folder>
        </folder>
        <folder name="AnnotationTypes">
<!--            <file name="CurrentPosition.xml" url="CurrentPosition.xml"/>-->
            <file name="NodePosition.xml" url="NodePosition.xml"/>
            <file name="CallSite.xml" url="CallSite.xml"/>
            <file name="CalledSite.xml" url="CalledSite.xml"/>
            
            <file name="NodePositionCurrent.xml" url="NodePositionCurrent.xml"/>
            <file name="CallSiteCurrent.xml" url="CallSiteCurrent.xml"/>
            <file name="CalledSiteCurrent.xml" url="CalledSiteCurrent.xml"/>
        </folder>
        <folder name="FontsColors">
            <folder name="NetBeans">
                <folder name="Defaults">
                    <file name="igv-sourceview-colorings.xml" url="igv-sourceview-colorings.xml">
                        <attr name="SystemFileSystem.localizingBundle" stringvalue="org.graalvm.visualizer.source.resources.Bundle"/>
                        <attr name="nbeditor-settings-ColoringType" stringvalue="highlight"/>
                    </file>
                </folder>
            </folder>
        </folder>
        <folder name="Toolbars">
            <folder name="Default">
                <file name="Separator-before-graph.instance">
                    <attr name="instanceClass" stringvalue="javax.swing.JSeparator"/>
                    <attr name="position" intvalue="25000"/>"
                </file>
                <file name="graph-select-nodes">
                    <attr name="position" intvalue="25100"/>"
                </file>
                <file name="graph-extract-nodes">
                    <attr name="position" intvalue="25200"/>"
                </file>
                <file name="Separator-before-track.instance">
                    <attr name="instanceClass" stringvalue="javax.swing.JSeparator"/>
                    <attr name="position" intvalue="25300"/>"
                </file>
                <file name="graph-track-nodes.instance">
                    <attr name="instanceCreate" newvalue="org.graalvm.visualizer.source.impl.editor.TrackNodesAction"/>
                    <attr name="position" intvalue="25400"/>"
                </file>
            </folder>
        </folder>
    </folder>
    <folder name="Windows2">
        <folder name="Modes">  
            <folder name="explorer">
                <file name="SourceCollectionTopComponent.wstcref" url="sourcesview.wstcref">
                    <attr name="position" intvalue="9000"/>
                </file>
            </folder>
        </folder>
    </folder>
</filesystem>
